{
  "name": "pmndrs-custom",
  "icon": "assets/pmndrs.png",
  "displayName": "poimandres custom",
  "description": "poimandres custom dark theme",
  "version": "0.3.7",
  "keywords": [
    "poimandres",
    "pmndrs",
    "vsc",
    "dark",
    "theme"
  ],
  "files": [
    "index.js"
  ],
  "scripts": {
    "dev": "node src/watcher"
  },
  "author": "Aidan MacMillan",
  "publisher": "Aidan MacMillan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AidanMacMillan/poimandres-theme-custom/issues"
  },
  "homepage": "https://github.com/drcmda/poimandres-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AidanMacMillan/poimandres-theme-custom.git"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "poimandres-custom",
        "uiTheme": "vs-dark",
        "path": "./themes/poimandres-color-theme.json"
      },
      {
        "label": "poimandres-custom-noitalics",
        "uiTheme": "vs-dark",
        "path": "./themes/poimandres-color-theme-noitalics.json"
      },
      {
        "label": "poimandres-custom-storm",
        "uiTheme": "vs-dark",
        "path": "./themes/poimandres-color-theme-storm.json"
      },
      {
        "label": "poimandres-custom-noitalics-storm",
        "uiTheme": "vs-dark",
        "path": "./themes/poimandres-color-theme-noitalics-storm.json"
      }
    ]
  },
  "devDependencies": {
    "filewatcher": "^3.0.1",
    "prettier": "^2.2.1",
    "sharp": "^0.28.0"
  }
}
